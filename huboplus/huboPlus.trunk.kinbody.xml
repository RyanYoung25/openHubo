<?xml version="1.0" encoding="utf-8"?>

<KinBody name="Trunk" makejoinedlinksadjacent="true">
	<modelsdir>models</modelsdir>
	<Body name="Torso" type="dynamic">
		<!-- Torso Base to Center of Mass -->
		<Translation>-0.010767979237 -0.001171538995 -0.063585314267</Translation>
		<Geom type="trimesh" render="true">
			<!-- Torso Center of Mass to Base -->
			<Translation>0.010767979237 0.001171538995 0.063585314267</Translation>
			<Render>Body_Torso.wrl 1</Render>
        </Geom>
		<Mass type="custom">
			<total>4.198979198860</total>
			<inertia>
				0.054247359920 -0.000016371895 0.004937384413
				-0.000016371895 0.033546508412 -0.000266457368
				0.004937384413 -0.000266457368 0.040330069994
			</inertia>
		</Mass>
    </Body>
	
	<Body name="Hip" type="dynamic">
        <offsetfrom>Torso</offsetfrom>
		<!-- Torso Center of Mass to Base -->
		<Translation>0.010767979237 0.001171538995 0.063585314267</Translation>
		<!-- Torso Base to Hip Base -->
        <Translation>0.000007742023 -0.000013409579 -0.196500048413</Translation>
		<!-- Hip Base to Center of Mass -->
		<Translation>-0.014522910106 -0.000034266017 -0.035304579825</Translation>
        <Geom type="trimesh">
			<!-- Hip Center of Mass to Base -->
			<Translation>0.014522910106 0.000034266017 0.035304579825</Translation>
            <Render>Body_HY.wrl 1</Render>
        </Geom>
		<Mass type="custom">
			<total>3.147415232298</total>
			<inertia>
				0.014660803864 0.000016332949 -0.000124216115
				0.000016332949 0.004395179328 0.000055619210
				-0.000124216115 0.000055619210 0.016007182800
			</inertia>
		</Mass>
	</Body>
	
	<Joint name="TY" type="hinge">
        <Body>Torso</Body>
        <Body>Hip</Body>
        <offsetfrom>Hip</offsetfrom>
        <axis>0 0 1</axis>
		<anchor>0.014522910106 0.000034266017 0.035304579825</anchor>
        <limitsdeg>-90 90</limitsdeg>
    </Joint>

	<adjacent>Torso Hip</adjacent>
    <Body name="Neck" type="dynamic">
        <offsetfrom>Torso</offsetfrom>
    </Body>

    <Joint name="HY" type="hinge">
        <Body>Torso</Body>
        <Body>Neck</Body>
        <offsetfrom>Neck</offsetfrom>
        <axis>0 0 1</axis>
        <limitsdeg>-90 90</limitsdeg>
    </Joint>

    <Body name="NeckPR" type="dynamic">
        <offsetfrom>Neck</offsetfrom>
        <Translation>0 0 .122</Translation>
        <Translation>0.005494756 0.000506882 0.048924021</Translation>
        <Geom type="trimesh">
            <render>Body_UJoint.wrl 1</render>
        </Geom>
    </Body>

    <Joint name="HR" type="hinge">
        <Body>Neck</Body>
        <Body>NeckPR</Body>
        <offsetfrom>NeckPR</offsetfrom>
        <axis>1 0 0</axis>
        <limitsdeg>-30 30</limitsdeg>
    </Joint>

    <Body name="Head" type="dynamic">
        <offsetfrom>NeckPR</offsetfrom>
        <Geom type="trimesh">
            <Translation>0 0 -.122</Translation>
            <Translation>0 0 .0425</Translation>
            <Render>Body_Head.wrl 1</Render>
            <data>convhull_Head.wrl 1</data>
        </Geom>
    </Body>

    <Joint name="HP" type="hinge">
        <Body>NeckPR</Body>
        <Body>Head</Body>
        <offsetfrom>Head</offsetfrom>
        <axis>0 1 0</axis>
        <limitsdeg>-30 30</limitsdeg>
    </Joint>

</KinBody>
