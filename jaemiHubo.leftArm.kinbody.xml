<?xml version="1.0" encoding="utf-8"?>

<!-- left arm kinematic tree -->
<!-- The torso is defined in multiple places, weventually need to find a good structure to join arms-->
<KinBody name="leftArm" makejoinedlinksadjacent="true">
<modelsdir>models</modelsdir>
    <Body name="Torso_dummy1" type="dynamic">
    </Body>
    <Body name="leftArm1" type="dynamic">
        <offsetfrom>Torso_dummy1</offsetfrom>
        <Translation>0 0.1425 0</Translation>
        <Geom type="sphere" render="false">
            <Translation>0 .064 0 </Translation>
            <radius>.080</radius>
        </Geom>
        <Geom type="trimesh">
            <Render>Left_Shoulder_Pitch.wrl 1</Render>
        </Geom>
    </Body>
    <Joint name="LSP" type="hinge">
        <Body>Torso_dummy1</Body>
        <Body>leftArm1</Body>
        <offsetfrom>leftArm1</offsetfrom>
        <axis>0 -1 0</axis>
        <limitsdeg>-180 180</limitsdeg>
    </Joint>
    <Body name="leftArm2" type="dynamic">
        <offsetfrom>leftArm1</offsetfrom>
        <Translation>.025 .072 0</Translation>
        <Geom type="cylinder" render="false">
            <Translation>-0.026 0 -0.038</Translation>
            <RotationAxis>1 0 0 90</RotationAxis>
            <radius>0.045</radius>
            <height>0.076</height>
        </Geom>
        <Geom type="trimesh">
            <Render>Left_Shoulder_Roll.wrl 1</Render>
        </Geom>
    </Body>
    <Joint name="LSR" type="hinge">
        <Body>leftArm1</Body>
        <Body>leftArm2</Body>
        <offsetfrom>leftArm2</offsetfrom>
        <axis>1 0 0</axis>
        <limitsdeg>-113 113</limitsdeg>
    </Joint>
    <Body name="leftArm3" type="dynamic">
        <offsetfrom>leftArm2</offsetfrom>
        <Translation>-0.0269 0.0 -0.0245</Translation>
        <Geom type="cylinder" render="false">
            <Translation>0 0 -0.073</Translation>
            <RotationAxis>1 0 0 90</RotationAxis>
            <radius>0.045</radius>
            <height>0.146</height>
        </Geom>
        <Geom type="sphere" render="false">
            <Translation>0.010 0.000 -0.146</Translation>
            <radius>.045</radius>
        </Geom>
        <Geom type="trimesh">
            <Render>Left_Shoulder_Yaw.wrl    1</Render>
        </Geom>
        <mass type="custom">
            <total>1</total>
        </mass>
    </Body>
    <Joint name="LSY" type="hinge">
        <Body>leftArm2</Body>
        <Body>leftArm3</Body>
        <offsetfrom>leftArm3</offsetfrom>
        <axis>0 0 1</axis>
        <limitsdeg>-157 157</limitsdeg>
    </Joint>
    <Body name="leftArm4" type="dynamic">
        <offsetfrom>leftArm3</offsetfrom>
        <Translation>.022279 0.0115 -.154441</Translation>
        <Geom type="cylinder" render="false">
            <Translation>-0.022 -0.012 -0.025</Translation>
            <RotationAxis>1 0 0 90</RotationAxis>
            <radius>0.044</radius>
            <height>0.051</height>
        </Geom>
        <Geom type="sphere" render="false">
            <Translation>-0.018 -0.011 0</Translation>
            <radius>0.049</radius>
        </Geom>
        <Geom type="trimesh">
            <Render>Left_Elbow_Pitch.wrl    1</Render>
        </Geom>
    </Body>
    <Joint name="LEP" type="hinge">
        <Body>leftArm3</Body>
        <Body>leftArm4</Body>
        <offsetfrom>leftArm4</offsetfrom>
        <axis>0 -1 0</axis>
        <limitsdeg>-20 150</limitsdeg>
    </Joint>
    <Body name="leftArm5" type="dynamic">
        <offsetfrom>leftArm4</offsetfrom>
        <Translation>-.022001 -.0115 -.041</Translation>
        <Geom type="cylinder" render="false">
            <Translation>0 0 -.064</Translation>
            <RotationAxis>1 0 0 90</RotationAxis>
            <radius>0.045</radius>
            <height>0.127</height>
        </Geom>
        <Geom type="trimesh">
            <Render>Right_Wrist_Yaw.wrl    1</Render>
        </Geom>
    </Body>
    <Joint name="LWY" type="hinge">
        <Body>leftArm4</Body>
        <Body>leftArm5</Body>
        <offsetfrom>leftArm5</offsetfrom>
        <axis>0 0 1</axis>
        <limitsdeg>-160 160</limitsdeg>
    </Joint>

    <Body name="leftUJoint" type="dynamic">
       <offsetfrom>leftArm5</offsetfrom> 
       <Translation>0 0 -.122</Translation> 
       <Geom type="box" render="false">
           <extents>.01 .01 .01</extents>
       </Geom>
        <Geom type="trimesh">
            <Render>Right_Wrist_UJoint.wrl    1</Render>
        </Geom>
    </Body>

    <Joint name="LWR" type="hinge">
        <Body>leftArm5</Body>
        <Body>leftUJoint</Body>
        <offsetfrom>leftUJoint</offsetfrom>
        <axis>-1 0 0</axis>
        <limitsdeg>-30 30</limitsdeg>
    </Joint>

    <Body name="leftPalm" type="dynamic">
       <offsetfrom>leftUJoint</offsetfrom> 
        <Translation>0 0 0</Translation> 
        <Geom type="trimesh">
            <Data>collision_Left_Palm.wrl    1</Data>
            <Render>Left_Palm.wrl    1</Render>
        </Geom>
    </Body>

    <Joint name="LWP" type="hinge">
        <Body>leftUJoint</Body>
        <Body>leftPalm</Body>
        <offsetfrom>leftPalm</offsetfrom>
        <axis>0 1 0</axis>
        <limitsdeg>-30 30</limitsdeg>
    </Joint>
    <Body name="leftPalmShell" type="dynamic" >
        <offsetfrom>leftPalm</offsetfrom> 
        <!--From Pro/E transformation matrix-->
        <Translation>0.009326346	-0.009307222	-0.060858088</Translation>
        <RotationMat>-.5 -.5 .7071 .5 .5 .7071 -.7071 .7071 0</RotationMat>
        <Geom type="trimesh">
           <Data>Left_Palm_Shell.wrl    .001</Data>
        </Geom>
    </Body>
    <Joint name="leftPalmBody" type="hinge" enabled="false">
        <Body>leftPalm</Body>
        <Body>leftPalmShell</Body>
        <offsetfrom>leftPalmShell</offsetfrom>
        <axis>1 0 0</axis>
        <limitsdeg>0 0</limitsdeg>
    </Joint>
    <adjacent>leftArm5 leftPalm</adjacent>
    <adjacent>leftArm3 leftArm5</adjacent>
    <adjacent>leftUJoint leftPalm</adjacent>
</KinBody>
