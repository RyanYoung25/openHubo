<?xml version="1.0" encoding="utf-8"?>

<!-- Left arm kinematic body of the Jaemi Hubo humanoids -->

<KinBody name="LeftArm">
    <Body name="Torso" type="dynamic">
        <Translation>0.03 0.7 0</Translation>
        <Geom type="trimesh">
            <Data>models/VRML/KHR4_Torso.wrl 1.0</Data>
            <Render>models/VRML/KHR4_Torso.wrl    1.0</Render>
        </Geom>
        <mass type="custom">
            <total>1</total>
        </mass>
    </Body>
    <Body name="Larm1" type="dynamic">
        <offsetfrom>Torso</offsetfrom>
        <Translation>0.025 0.029 -0.28</Translation>
        <Geom type="trimesh">
            <Data>models/VRML/L_SHOULDER_PITCH.wrl 1.0</Data>
            <Render>models/VRML/L_SHOULDER_PITCH.wrl 1.0</Render>
        </Geom>
        <mass type="custom">
            <total>1</total>
        </mass>
    </Body>

    <Joint name="LSP" type="hinge">
        <Body>Torso</Body>
        <Body>Larm1</Body>
        <offsetfrom>Larm1</offsetfrom>
        <axis>1 0 0</axis>
        <!-- Settings from WAM, will need to be updated-->
        <limitsdeg>-150 150</limitsdeg>
        <maxvel>1.5708</maxvel>
        <weight>1.92154</weight>
        <resolution>0.25</resolution>
    </Joint>
    <Body name="Larm2" type="dynamic">
        <offsetfrom>Larm1</offsetfrom>
        <Translation>0    0    0</Translation>
        <RotationMat>1 0 0 0 0 1 0 -1 0</RotationMat>
        <Geom type="trimesh">
            <Data>models/VRML/Larm2.wrl    1.0</Data>
            <Render>models/VRML/Larm2.wrl    1.0</Render>
        </Geom>
        <mass type="custom">
            <total>4.8487</total>
        </mass>
    </Body>
    <Joint name="LSR" type="hinge">
        <Body>Larm1</Body>
        <Body>Larm2</Body>
        <offsetfrom>Larm2</offsetfrom>
        <axis>1 0 0</axis>
        <limitsdeg>-113 113</limitsdeg>
        <weight>0.91739941</weight>
        <maxvel>1.0472</maxvel>
        <resolution>0.5</resolution>
    </Joint>
    <Body name="Larm3" type="dynamic">
        <offsetfrom>Larm2</offsetfrom>
           
        <Geom type="trimesh">
            <Data>models/VRML/Larm3.wrl    1.0</Data>
            <Render>models/VRML/Larm3.wrl    1.0</Render>
        </Geom>
        <mass type="custom">
            <total>1.7251</total>
        </mass>
    </Body>
    <Joint name="LSY" type="hinge">
        <Body>Larm2</Body>
        <Body>Larm3</Body>
        <offsetfrom>Larm3</offsetfrom>
        <axis>0 0 1</axis>
        <limitsdeg>-157 157</limitsdeg>
        <weight>0.882397</weight>
        <maxvel>2.0944</maxvel>
        <resolution>0.5</resolution>
    </Joint>
    <Body name="Larm4" type="dynamic">
        <offsetfrom>Larm3</offsetfrom>
        <Translation>0.045 0 0.55</Translation>
        <RotationMat>1 0 0 0 0 1 0 -1 0</RotationMat>
        <Geom type="trimesh">
            <Data>models/VRML/Larm4.wrl    1.0</Data>
            <Render>models/VRML/Larm4.wrl    1.0</Render>
        </Geom>
        <mass type="custom">
            <total>0.4809</total>
        </mass>
    </Body>
    <Joint name="LEP" type="hinge">
        <Body>Larm3</Body>
        <Body>Larm4</Body>
        <offsetfrom>Larm4</offsetfrom>
        <axis>0 1 0</axis>
        <limitsdeg>-50 180</limitsdeg>
        <weight>0.45504</weight>
        <maxvel>2.0944</maxvel>
        <resolution>1</resolution>
    </Joint>
    <Body name="Larm5" type="dynamic">
        <offsetfrom>Larm4</offsetfrom>
        <Translation>-0.045 0 0</Translation>
        <RotationMat>1 0 0 0 0 -1 0 1 0</RotationMat>
        <Geom type="trimesh">
            <Data>models/VRML/Larm5.wrl    1.0</Data>
            <Render>models/VRML/Larm5.wrl    1.0</Render>
        </Geom>
        <mass type="custom">
            <total>1.6015</total>
        </mass>
    </Body>
    <Joint name="LWY" type="hinge">
        <Body>Larm4</Body>
        <Body>Larm5</Body>
        <offsetfrom>Larm5</offsetfrom>
        <axis>0 0 1</axis>
        <limitsdeg>-275 75</limitsdeg>
        <weight>0.40141</weight>
        <maxvel>4.1888</maxvel>
        <resolution>1.11</resolution>
    </Joint>

    <Body name="Larm6" type="dynamic">
        <offsetfrom>Larm5</offsetfrom>
        <Translation>0 0 0.3</Translation>
        <RotationMat>1 0 0 0 0 1 0 -1 0</RotationMat>
        <Geom type="trimesh">
            <Data>models/VRML/Larm6.wrl    1.0</Data>
            <Render>models/VRML/Larm6.wrl    1.0</Render>
        </Geom>
        <mass type="custom">
            <total>0.3067</total>
        </mass>
    </Body>

    <Joint name="Wrist_Pitch" type="hinge">
        <Body>Larm5</Body>
        <Body>Larm6</Body>
        <offsetfrom>Larm6</offsetfrom>
        <axis>0 0 1</axis>
        <limitsdeg>-90 90</limitsdeg>
        <weight>0.22841245</weight>
        <maxvel>4.1888</maxvel>
        <resolution>1.62</resolution>
    </Joint>
    <Body name="Larm7" type="dynamic">
        <offsetfrom>Larm6</offsetfrom>
        <Translation>0.0    0.0    0.0</Translation>
        <RotationMat>1 0 0 0 0 -1 0 1 0</RotationMat>
        <Geom type="trimesh">
            <data>models/VRML/Larm7.wrl 1.0</data>
            <Render>models/VRML/Larm7.wrl 1.0</Render>
        </Geom>
        <mass type="custom">
            <total>1.18</total>
        </mass>
    </Body>

    <Joint name="Wrist_Roll" type="hinge">
        <Body>Larm6</Body>
        <Body>Larm7</Body>
        <offsetfrom>Larm7</offsetfrom>
        <axis>0 0 1</axis>
        <limitsdeg>-172 172</limitsdeg>
        <weight>0.20178</weight>
        <maxvel>1.0472</maxvel>
        <resolution>1.62</resolution>
    </Joint>

    <adjacent>Larm1 Larm3</adjacent>
    <adjacent>Larm4 Larm6</adjacent>
    <adjacent>Larm4 Larm7</adjacent>
</KinBody>
