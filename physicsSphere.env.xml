<?xml version="1.0" encoding="utf-8"?>

<Environment>

    <KinBody name="testSphere" makejoinedlinksadjacent="true">

        <Body name="Sphere" type="dynamic">
            <Translation>0 0 .5</Translation>
            <Geom type="sphere" render="true">
                <Translation>.1 0 0</Translation>
                <radius>.1</radius>
                <Diffusecolor>.8 .8 .5</Diffusecolor>
            </Geom>
            <Mass type="custom">
                <total>1</total>
                <com>-.1 0 0</com>
                <inertia>1 0 0 0 10 0 0 0 3</inertia>
            </Mass>
        </Body>

    </KinBody>

    <KinBody name="floor">
        <Body name="plate" type="static">
            <translation>0 0 -.1</translation>
            <Geom type="box">
                <RotationAxis>0 1 0 0</RotationAxis>
                <extents>1 1 .1</extents>
            </Geom>
        </Body>
    </KinBody>

</Environment>
