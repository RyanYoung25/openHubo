<Environment>

    <robot file="gripfixture.robot.xml"/>

    <KinBody name="rod">
        <Body name="rod1" type="dynamic">
            <translation>.019 0.0345 -0.12976</translation>
            <Geom type="cylinder">
                <RotationAxis>0 0 1 90</RotationAxis>
                <radius>.03</radius>
                <height>1</height>
                <diffuseColor>.8 .2 .2</diffuseColor>
            </Geom>
            <mass type="mimicgeom">
                <!--tune this with python-->
                <total>4.54</total>
            </mass>
        </Body>
    </KinBody>

    <physicsengine type="ode">
        <odeproperties>
            <friction>1</friction>
            <gravity>0 0 -9.8</gravity>
            <selfcollision>1</selfcollision>
            <erp>.5</erp>
            <cfm>.0001</cfm>
            <dcontactapprox>1</dcontactapprox>
        </odeproperties>
    </physicsengine>

</Environment>
